package com.sprting.annotations;

import org.springframework.context.support.ClassPathXmlApplicationContext;

public class AnnotationBeanScopeDemoApp {

	public static void main(String[] args) {
		
		ClassPathXmlApplicationContext applicationContext=new ClassPathXmlApplicationContext("applicationContex.xml");
		
		Coach coach=applicationContext.getBean("tennisCocah",Coach.class);
		Coach alphaCoach=applicationContext.getBean("tennisCocah",Coach.class);
		

	}

}
